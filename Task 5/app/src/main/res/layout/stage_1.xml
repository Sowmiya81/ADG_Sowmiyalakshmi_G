<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="37dp"
        android:text="Options"
        android:textColor="@color/black"
        android:textSize="35dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="23dp"
        android:text="Important"
        android:textColor="#6b6b6b"
        android:textSize="15dp"
        android:textStyle="bold" />

    <include
        android:id="@+id/imp1"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <include
        android:id="@+id/imp2"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/imp1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />


    <include
        android:id="@+id/imp3"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/imp2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imp3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="35dp"
        android:text="Files"
        android:textColor="#6b6b6b"
        android:textSize="15dp"
        android:textStyle="bold" />

    <include
        android:id="@+id/file1"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <include
        android:id="@+id/file2"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/file1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <include
        android:id="@+id/file3"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/file2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <include
        android:id="@+id/file4"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/file3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-4dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/file4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="36dp"
        android:text="Others"
        android:textColor="#6b6b6b"
        android:textSize="15dp"
        android:textStyle="bold" />

    <include
        android:id="@+id/other1"
        layout="@layout/stage_1_cardview"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp" />


</RelativeLayout>